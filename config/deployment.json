{
    "modulesContent": {
      "$edgeAgent": {
        "properties.desired": {
          "schemaVersion": "1.0",
          "runtime": {
            "type": "docker",
            "settings": {
              "minDockerVersion": "v1.25",
              "loggingOptions": "",
              "registryCredentials": {
                "registryName": {
                  "username": "$CONTAINER_REGISTRY_USERNAME",
                  "password": "$CONTAINER_REGISTRY_PASSWORD",
                  "address": "$CONTAINER_REGISTRY_ADDRESS"
                }
              }
            }
          },
          "systemModules": {
            "edgeAgent": {
              "type": "docker",
              "settings": {
                "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
                "createOptions": ""
              }
            },
            "edgeHub": {
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
                "createOptions": ""
              },
              "env": {
                "OptimizeForPerformance": {
                  "value": "false"
                }
              }
            }
          },
          "modules": {
            "stream-image": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "env": {
                "IMAGE_PROCESSING_ENDPOINT": {
                  "value": "http://pest-detection-service:80/image"
                },
                "SHOW_IMAGE": {
                  "value": "True"
                },
                "VERBOSE": {
                  "value": "True"
                },
                "RESIZE_WIDTH": {
                  "value": "640"
                },
                "RESIZE_HEIGHT": {
                  "value": "640"
                },
                "ANNOTATE": {
                  "value": "True"
                }
              },
              "settings": {
                "image": "demopestdetection.azurecr.io/demostreamimage",
                "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"5012/tcp\":[{\"HostPort\":\"5012\"}]}}}"
              }
            },
            "pest-detection-service": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "demopestdetection.azurecr.io/demopestdetection",
                "createOptions": ""
              }
            }
          }
        }
      },
      "$edgeHub": {
        "properties.desired": {
          "schemaVersion": "1.0",
          "routes": {
            "CameraCaptureToIoTHub": "FROM /messages/modules/stream-image/outputs/output1 INTO $upstream",
            "CameraCaptureGAToIoTHub": "FROM /messages/modules/StreamImageGA/outputs/* INTO $upstream"
          },
          "storeAndForwardConfiguration": {
            "timeToLiveSecs": 7200
          }
        }
      }
    }
  }